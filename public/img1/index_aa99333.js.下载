var Swiper=require("cn:components/swiper/swiper");$(function(){function i(){var i=$("a[dynamic]");i.each(function(i,e){var n=$(e),o=n.attr("data-href"),a=n.attr("href");e.href=t?o:a})}function e(){if(window.getViewport().width<=500){var e=new Swiper(".swiper-container",{pagination:".swiper-pagination",mode:"horizontal",paginationClickable:!0,mousewheelControl:!0,visibilityFullFit:!0,calculateHeight:!0,onSwiperCreated:function(){$(".service-choose").addClass("active")}});e.reInit(),r=window.getViewport().width}else 500>=r&&window.location.reload(),r=window.getViewport().width;i()}var n=window.isMobile()&&$(window).width()<500,o=!window.isMobile()&&$(window).width()<500,t=o||n;if(t){var a=new Swiper(".swiper-container",{pagination:".swiper-pagination",mode:"horizontal",paginationClickable:!0,visibilityFullFit:!0,onSwiperCreated:function(){$(".service-choose").addClass("active")}});a.init()}$(".question-item").bind("click",function(i){i.preventDefault();var e=$(this);t&&(e.hasClass("open")?setTimeout(function(){e.children(".question-list-detail").slideUp(),e.removeClass("open")},0):setTimeout(function(){e.children(".question-list-detail").slideDown(),e.addClass("open")},0))}),$(".question-list-detail").on("click",".question-link",function(i){i.stopPropagation()}),$(".container li").on("mouseenter",function(){$(this).children("a").addClass("hover")}).on("mouseleave",function(){$(this).children("a").removeClass("hover")}),$("#meizu-header-link-product-mx,#meizu-header-link-product-meilan,#meizu-header-link-product-audio,#meizu-header-link-product-accessory,.meizu-header-sub").on("mouseenter",function(){$("#meizu-header-container").css("background","#fff")}).on("mouseleave",function(){$("#meizu-header-container").css("background","transparent")});var r=1240;$(window).resize(window.throttle(e,200)),e()});